<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>
<fx:root prefHeight="600.0" prefWidth="800.0" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.171"
         xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="MenuItems">
                            <items>
                                <MenuItem mnemonicParsing="false" text="MenuItem"/>
                                <MenuItem disable="true" mnemonicParsing="false" text="Disabled"/>
                                <SeparatorMenuItem mnemonicParsing="false"/>
                                <MenuItem mnemonicParsing="false" text="SeparatorMenuItem"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="CheckMenuItems">
                            <items>
                                <CheckMenuItem mnemonicParsing="false" text="CheckMenuItem"/>
                                <CheckMenuItem mnemonicParsing="false" selected="true" text="Selected"/>
                                <CheckMenuItem disable="true" mnemonicParsing="false" text="Disabled"/>
                                <CheckMenuItem disable="true" mnemonicParsing="false" selected="true"
                                               text="Selected Disabled"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="RadioMenuItems">
                            <items>
                                <RadioMenuItem mnemonicParsing="false" text="RadioMenuItem"/>
                                <RadioMenuItem mnemonicParsing="false" selected="true" text="Selected"/>
                                <RadioMenuItem disable="true" mnemonicParsing="false" text="Disabled"/>
                                <RadioMenuItem disable="true" mnemonicParsing="false" selected="true"
                                               text="Selected Disabled"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Menus">
                            <items>
                                <Menu mnemonicParsing="false" text="Menu">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="Action 1"/>
                                    </items>
                                </Menu>
                                <Menu disable="true" mnemonicParsing="false" text="Disabled"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <ToolBar>
                    <items>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ToolBar"/>
                        <Button mnemonicParsing="false">
                            <graphic>
                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="18.0" stroke="BLACK"
                                           strokeType="INSIDE" width="18.0"/>
                            </graphic>
                        </Button>
                        <Button disable="true" mnemonicParsing="false">
                            <graphic>
                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ff1f1f" height="18.0" stroke="BLACK"
                                           strokeType="INSIDE" width="18.0"/>
                            </graphic>
                        </Button>
                    </items>
                </ToolBar>
            </children>
        </VBox>
    </top>
    <bottom>
        <ButtonBar BorderPane.alignment="CENTER">
            <buttons>
                <Button mnemonicParsing="false">
                    <graphic>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="18.0" stroke="BLACK"
                                   strokeType="INSIDE" width="18.0"/>
                    </graphic>
                </Button>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ButtonBar"/>
            </buttons>
        </ButtonBar>
    </bottom>
    <center>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Accordion">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Accordion prefHeight="487.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0"
                                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                           AnchorPane.topAnchor="0.0">
                                    <panes>
                                        <TitledPane animated="false" text="TitledPane">
                                            <content>
                                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                            prefWidth="200.0"/>
                                            </content>
                                        </TitledPane>
                                        <TitledPane animated="false" text="TitledPane">
                                            <content>
                                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                            prefWidth="200.0"/>
                                            </content>
                                        </TitledPane>
                                        <TitledPane animated="false" text="TitledPane">
                                            <content>
                                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                            prefWidth="200.0"/>
                                            </content>
                                        </TitledPane>
                                    </panes>
                                </Accordion>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="ScrollPane">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ScrollPane layoutX="97.0" layoutY="130.0" prefHeight="487.0" prefWidth="640.0"
                                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <content>
                                        <AnchorPane>
                                            <children>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" height="2000.0" stroke="BLACK"
                                                           strokeType="INSIDE" width="2000.0"
                                                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                    <fill>
                                                        <LinearGradient endX="1.0" endY="1.0">
                                                            <stops>
                                                                <Stop color="BLACK"/>
                                                                <Stop color="WHITE" offset="1.0"/>
                                                            </stops>
                                                        </LinearGradient>
                                                    </fill>
                                                </Rectangle>
                                            </children>
                                        </AnchorPane>
                                    </content>
                                </ScrollPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
    <left>
        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
                   BorderPane.alignment="CENTER">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <ListView fx:id="listView" layoutX="-71.0" layoutY="14.0" prefHeight="253.0" prefWidth="158.0"
                                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                  AnchorPane.topAnchor="0.0"/>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <TreeView fx:id="treeView" layoutX="-56.0" layoutY="14.0" prefHeight="252.0" prefWidth="158.0"
                                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                  AnchorPane.topAnchor="0.0"/>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </left>
</fx:root>
